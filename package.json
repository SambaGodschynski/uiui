{
    "name": "uiui",
    "publisher": "JohannesUnger",
    "displayName": "UiUi",
    "description": "UiUi for VS Code",
    "icon": "resources/Logo.png",
    "repository": "https://github.com/werckme/uiui-codext",
    "version": "0.0.2",
    "engines": {
        "vscode": "^1.38.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.uiui.inspector"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "views": {},
        "viewsWelcome": [],
        "commands": [
            {
                "command": "extension.uiui.inspector",
                "title": "UiUi: Open Panel",
                "icon": {
                    "light": "./resources/logo_light.svg",
                    "dark": "./resources/logo_dark.svg"
                }
            }
        ],
        "menus": {
            "editor/title": [
                {
                    "when": "resourceFilename =~ /.*uiui\\.json$/",
                    "command": "extension.uiui.inspector",
                    "group": "navigation"
                }
            ],
            "editor/context": []
        },
        "languages": [],
        "grammars": [],
        "configuration": {},
        "jsonValidation": [
            {
                "fileMatch": "*.uiui.json",
                "url": "./uiui.schema.json"
            }
        ]
    },
    "scripts": {
        "compile": "tsc -p ./ && npm run build-webviewapp",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile",
        "test": "node ./out/test/runTest.js",
        "build-webviewapp": "webpack --config webViewApp.webpack.config.js",
        "webview-dev": "webpack --config webViewApp.webpack.config.js --watch",
        "vscode:prepublish": "webpack --mode production",
        "webpack": "webpack --mode development",
        "webpack-dev": "webpack --mode development --watch",
        "test-compile": "tsc -p ./",
        "publish": "vsce package && vsce publish",
        "package": "npm run build-webviewapp && vsce package"
    },
    "devDependencies": {
        "@babel/core": "^7.6.3",
        "@babel/preset-env": "^7.6.3",
        "@babel/preset-react": "^7.6.3",
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.6",
        "@types/node": "^10.12.21",
        "@types/vscode": "^1.38.0",
        "babel-loader": "^8.0.6",
        "glob": "^7.1.4",
        "mocha": "^10.1.0",
        "regenerator-runtime": "^0.13.7",
        "ts-loader": "^6.2.2",
        "tslint": "^5.12.1",
        "typescript": "^3.3.1",
        "vsce": "^2.6.7",
        "vscode-test": "^1.2.0",
        "webpack": "^5.69.1",
        "webpack-cli": "^4.9.2"
    },
    "dependencies": {
        "@types/lodash": "^4.14.144",
        "@types/mathjs": "^9.4.2",
        "lodash": "^4.17.21",
        "mathjs": "^13.2.0",
        "react": "^16.10.2",
        "react-dom": "^16.10.2",
        "taskkill": "^3.0.0"
    }
}
